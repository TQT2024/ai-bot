{
  "expo": {
    "scheme": "ai-bot",
    "name": "ai-bot",
    "slug": "ai-bot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "assets/icon.png",
          "color": "#ffffff",
          "sounds": [
            "assets/mixkit-long-pop-2358.wav"
          ]
        }
      ],
      [
        "expo-google-sign-in",
        {
          "clientId": "759317752188-q3mlq3uis4m6iu7bk8553pvv2bj45irn.apps.googleusercontent.com"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anhtuatus.aibot",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true,
      "package": "com.anhtuatus.aibot",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dc47898a-3ccb-4a7c-ba9b-01a762020ee4"
      }
    }
  }
}
